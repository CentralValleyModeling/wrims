        value 11.9 }!3.5% of 256 taf from CalsimII recalculated to account for difference in CUAW and AW
    case april  {
        condition month==apr
        sum(i=0,6,1) (AWO_62_NA6(i)+AWR_62_NA6(i))*cfs_taf(i) }
    case otherwise {
        condition always
        value ann_AW_62_NA6dv(-1) }
}

