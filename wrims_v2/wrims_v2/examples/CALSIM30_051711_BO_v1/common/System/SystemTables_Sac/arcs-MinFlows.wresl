! Cache Creek operational spill below Capay Diversion Dam
! estimated diversion demand at Capay Diversion Dam (99% of 20_NA1 demand)

define CacheAlloc {select allocation from CacheAllocation where wyt = wyt_sac}
define CacheAllocdv {alias CacheAlloc kind 'allocation-factor' units 'none'}

define estdvrdmd {value max(0.,.99*(AWR_20_NA1*(1.- RUFR_20_NA1) + AWO_20_NA1*(1.- RUFO_20_NA1))*(1. + RPF_20_NA1 - GPmin_20_NA1)*(1. + EVF_CCH030_20_NA1 + DPF_CCH030_20_NA1 + LFF_CCH030_20_NA1 + OSF_CCH030_20_NA1))}
define estdvrdmddv {alias estdvrdmd kind 'estimated-demand' units 'cfs'}

define MF_C_CCH030 {value .05*estdvrdmd}
define C_CCH030_MIF {std kind 'FLOW-MIN-INSTREAM' units 'CFS'}
define C_CCH030_ADD {std kind 'FLOW-ADDITIONAL-INSTREAM' units 'CFS'}

define MF_C_CCH012 {value .05*estdvrdmd}
