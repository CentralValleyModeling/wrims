SEQUENCE CYCLE1 {
    model   first
    order   1
    }

MODEL first { 

	define x {std kind 'CHANNEL' units 'CFS'}

	goal someGoal {
		lhs x
		case DryCritical {
			condition 5 > 9
			rhs 0.0
			lhs>rhs penalty -500.}
		case otherwise {
			condition always
			rhs 0.
			lhs>rhs penalty 100.}
	}	
}
