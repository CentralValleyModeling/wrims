!==============================
! 60S_PA2 - CSJWCD - Ag
!==============================
! deliveries
goal setEV_LJC010_60S_PA2 {EV_LJC010_60S_PA2= D_LJC010_60S_PA2 * EVF_LJC010_60S_PA2}
goal setEV_60S_PA2        {EV_60S_PA2       = EV_LJC010_60S_PA2}
 
goal setDP_LJC010_60S_PA2 {DP_LJC010_60S_PA2= D_LJC010_60S_PA2 * DPF_LJC010_60S_PA2}
goal setDP_60S_PA2        {DP_60S_PA2       = DP_LJC010_60S_PA2}

goal setLF_LJC010_60S_PA2 {LF_LJC010_60S_PA2= D_LJC010_60S_PA2 * LFF_LJC010_60S_PA2}
goal setLF_60S_PA2        {LF_60S_PA2       = LF_LJC010_60S_PA2}

goal setDL_LJC010_60S_PA2 {DL_LJC010_60S_PA2= EV_LJC010_60S_PA2 + DP_LJC010_60S_PA2 + LF_LJC010_60S_PA2 + OS_LJC010_60S_PA2}
goal setDL_60S_PA2	  {DL_60S_PA2       = EV_60S_PA2 + DP_60S_PA2 + LF_60S_PA2 + OS_60S_PA2}

goal setTW_60S_PA2        {TW_60S_PA2       = AWO_60S_PA2*TWFO_60S_PA2+AWR_60S_PA2*TWFR_60S_PA2}

! reuse < sum of tailwater and canal seepage to drains
! reuse < (rufr * awr) + (rufo * awo)
goal set1RU_60S_PA2       {RU_60S_PA2 < TW_60S_PA2 + LF_60S_PA2} 
goal set2RU_60S_PA2       {RU_60S_PA2 < RUFR_60S_PA2*AWR_60S_PA2 + RUFO_60S_PA2*AWO_60S_PA2} 
! applied water  = awr + awo
goal setAW_60S_PA2        {AW_60S_PA2 = AWR_60S_PA2 + AWO_60S_PA2} 
! riparian losse = factor * aw
goal setRP_60S_PA2        {RP_60S_PA2 = AW_60S_PA2 * RPF_60S_PA2} 



