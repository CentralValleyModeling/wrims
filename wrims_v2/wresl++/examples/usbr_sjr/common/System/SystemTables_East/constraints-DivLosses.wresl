! Delivery loss is 115.8 taf/year distributed Apr-Oct via AW pattern
define DL_63_NA3_sv {
    case AprthruOct {
        condition month >= Apr .or. month==Oct
        value AW_63_NA3_dist * 115.8 * taf_cfs }
    case otherwise {
        condition always
        value 0. }
}

