goal setDP_62_NA6         {DP_MCD055_62_NA6 = 0.9*0.75*DL_62_NA6 - 0.9*0.75*OS_62_NA6}
goal setLF_62_NA6         {LF_MCD055_62_NA6 = 0.9*0.25*DL_62_NA6 - 0.9*0.25*OS_62_NA6}
*/

