SEQUENCE CYCLE1 {
    model   first
    order   1
    }

SEQUENCE CYCLE2 {
    model   second
    order   2
    }	

SEQUENCE CYCLE3 {
    model   third
    order   3
    }

SEQUENCE CYCLE4 {
    model   fourth
    order   4
    }
	
MODEL first { 

	define[local] sv_1 { value 1 }
	define[local] dv_1 { std kind 'CHANNEL' units 'CFS'}
	define c { value 1 }
}

MODEL second { 

	define[local] sv_2 { value 2 }
	define dv_2_global { lower 400 upper 600 kind 'CHANNEL' units 'CFS'}
}

MODEL third { 

	define[local] as_3 { alias dv_2_global }	
	define[local] int_3 { integer lower 10 upper 20 kind 'integer' units 'CFS'}	
}

MODEL fourth { 

	define[local] sv_expr1 { value sv_1[first] + dv_2[second] }
	define sv_expr2 { value min(max(as_3[third],sv_2[second_not_exist]),sv_2[second] + dv_2_global[second]) }
	define sv_expr3 { value int(min(int_3[third],dv_1[first])) }
	
}
