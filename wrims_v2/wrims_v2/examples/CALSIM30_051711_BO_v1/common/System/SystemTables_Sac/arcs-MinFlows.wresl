define	C_OGN005_MIF	{std kind 'FLOW-MIN-INSTREAM' units 'CFS'}	
define	C_OGN005_ADD	{std kind 'FLOW-ADDITIONAL-INSTREAM' units 'CFS'}
define 	MF_OGN005 		{
	case apr {
    		condition   month == APR
    		value       10 }
  	case may  {
    		condition   month == MAY
    		value       12 }
    	case june  {
    		condition   month == JUN
    		value       10 }
    	case other {
    		condition always
    		value 8 }
}

